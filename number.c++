// write a c++ program to print int, float, double data type

#include<iostream>

using namespace std;

int main()

{
    int num1=1000;
    float num2=3.14;
    double num3=88.99;

    //declaring scientific number
    float f1 =35e3;
    double f2 =12E4;

    cout << num1 << endl;
    cout << num2 << endl;
    cout << num3 << endl;
    cout << f1 << endl;
    cout << f2 << endl;
}